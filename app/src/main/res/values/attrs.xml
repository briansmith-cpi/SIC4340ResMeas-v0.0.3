<resources>

    <declare-styleable name="VerticalBarViewStyleable">
        <attr name="range" format="integer" />
        <attr name="value" format="float" />
        <attr name="buffer" format="boolean" />
        <attr name="bias" format="integer" />
    </declare-styleable>

    <declare-styleable name="InputLayoutStyleable">
        <attr name="editor" format="boolean" />
        <attr name="spinner" format="boolean" />
        <attr name="box" format="boolean" />
        <attr name="text" format="string" />
        <attr name="unit" format="string" />
        <attr name="value" />
        <attr name="spn_list" format="reference" />
        <attr name="select" format="integer" />
        <attr name="edt_adjust" format="float" />
        <attr name="edt_max" format="float" />
        <attr name="edt_min" format="float" />
        <attr name="edt_length" format="integer" />
        <attr name="integer" format="boolean" />
        <attr name="check" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ShortInputLayoutStyleable">
        <attr name="text" />
        <attr name="unit" />
        <attr name="value" />
        <attr name="integer" />
        <attr name="length" format="integer" />
        <attr name="freeze" format="boolean" />
        <attr name="valueColorId" format="reference" />
    </declare-styleable>

    <declare-styleable name="EepromEditorLayoutStyleable">
        <attr name="run" format="boolean" />
        <attr name="specific" format="boolean" />
        <attr name="hex_address" format="string" />
        <attr name="hex_data" format="string" />
        <attr name="comment" format="string" />
    </declare-styleable>

    <declare-styleable name="LogLayoutStyleable">
        <attr name="title" format="string" />
        <attr name="text" />
        <attr name="unit" />
    </declare-styleable>
</resources>